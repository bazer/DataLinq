{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/[Tt]est*/**/*.csproj",
            "**/*[Tt]est*.csproj",
            "**/[Bb]enchmark*/**/*.csproj",
            "**/*[Bb]enchmark*.csproj",
            "**/[Bb]lazor*/**/*.csproj",
            "**/*[Bb]lazor*.csproj",
            "**/[Ee]xample*/**/*.csproj",
            "**/[Ss]ample*/**/*.csproj"
          ],
          "src": "."
        }
      ],
      "output": "api",
      "outputFormat": "apiPage"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "README.md",
          "*.md",
          "docs/**/*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "sitemap": {
      "baseUrl": "https://bazer.github.io/datalinq",
      "priority": 0.5,
      "changefreq": "daily"
    },
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [ "ExtractSearchIndex"],
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "DataLinq",
      "_appName": "DataLinq",
      "_gitContribute": "https://github.com/bazer/DataLinq",
      "_gitHubUrl": "https://github.com/bazer/DataLinq",
      "_enableSearch": true,
      "pdf": false
    }
  }
}